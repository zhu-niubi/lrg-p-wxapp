.container{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  .query {
    width: 610rpx;
    justify-content: space-between;
    height: 120rpx;
    margin-left: 70rpx;
    margin-right: 70rpx;
    line-height: 120rpx;
    border-bottom: 1rpx solid #EDEDED;
    font-size: 30rpx;
    display: flex;
    align-items: center
  }
  
  .bg-ededed{
      background-color:#EDEDED;
      width: 100%;
      min-height: 100vh;
  }
  
  .query .area {
    width: 100rpx;
    color:#000000;
    font-size: 32rpx;
    font-family: PingFang SC;
  }
  
  .query picker .pk {
    width: 410rpx;
    text-align: center;
    margin-right: 100rpx;
  }
  
  .query .clickTest {
      color: gray;
  }
  
  .icon-user {
    background: url('http://quantum-1255467040.cossh.myqcloud.com/miniProgram/user.png');
    width: 32rpx;
    height: 32rpx;
    background-size: cover
  }
  
  .icon-phone {
    background: url('http://quantum-1255467040.cossh.myqcloud.com/miniProgram/phone.png');
    width: 36rpx;
    height: 36rpx;
    background-size: cover
  }
  
  .icon-add {
    background: url('http://quantum-1255467040.cossh.myqcloud.com/miniProgram/address.png');
    width: 36rpx;
    height: 36rpx;
    background-size: cover
  }
  
  
  button.at {
    width: 530rpx;
    height: 60rpx;
    background-color: #464646;
    color: #fff;
    border-radius: 16rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    margin-top: 48rpx;
  }
  
  .content {
    padding: 26rpx 30rpx 0;
    font-size: 30rpx
  }
  
  .store {
    background-color: #333;
    border-radius: 10rpx;
    padding: 26rpx 28rpx 38rpx;
    font-size: 30rpx;
    margin-bottom: 20rpx
  }
  
  .store .s1 {
    font-size: 34rpx;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 40rpx
  }
  
  .store .s1 .icon-arrow {
    margin-top: 7rpx;
  }
  
  .store .s2 {
    margin-bottom: 44rpx
  }
  
  .store .s2 .part .userName {
    width: 204rpx
  }
  
  .store .s2 .part .icon-user {
    margin-right: 20rpx
  }
  
  .store .s2 .part .icon-phone {
    margin-right: 20rpx
  }
  
  .store .icon-add {
    margin-right: 20rpx
  }
  
  .down {
    width: 36rpx;
    height: 20rpx;
  }
  
  .viewDetail {
    width: 690rpx;
    margin: 20rpx auto;
  }
  
  .viewDetail .viewTit {
    display: flex;
    justify-content: center;
    background-color: #ff4713;
    padding: 10rpx 0;
    border-radius: 15rpx;
  }
  
  .viewDetail .viewTit .oLine {
    width: 6rpx;
    height: 43rpx;
    /* background-color: #ffba15; */
    margin-right: 20rpx;
    margin-left: 30rpx;
  }
  
  .viewDetail .viewTit .vi1 {
    color: #fff;
    font-size: 30rpx;
  }
  
  .viewBody {
    padding:30rpx;
    background-color: #fff;
    margin: 20rpx auto;
    border-radius: 15rpx;
  }
  .viewStore{
      background-color: #fff;
      margin: 20rpx auto;
      border-radius: 15rpx;
  }
  
  .viewCompany {
    color: #1A1A1A;
    font-size: 30rpx;
    font-weight: 500;
  }
  
  .viewMsg .viewName {
    color: #323232;
    font-size: 28rpx;
  }
  
  .viewMsg .ddf {
    align-items: center;
    height: 100rpx;
  }
  
  .viewMsg .viewPho {
    color: #323232;
    font-size: 28rpx;
    padding-left: 30rpx;
  }
  
  .viewAdd {
    color: #323232;
    font-size: 26rpx;
  }
  .click{
    width: 530rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 16rpx;
    background-color: #54BBFF;
    color: #fff;
    font-size: 28rpx;
    font-family: PingFang SC;
    margin-top: 34rpx;
  }
  .btn{
    width: 690rpx;
    height: 90rpx;
    line-height: 90rpx;
    margin-top: 30rpx;
    background-color: #4A4A4A;
    color: #fff;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, sans-serif;
  }
  .nav{
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
    border: 1px solid #ddd;
  }
  .tab{
    width: 50%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #ff4713;
    font-weight: bold;
  }
  .on{
    width: 50%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
  } 
  .line{
    width: 4rpx;
    height: 80rpx;
    background-color: #ddd;
  }
  .wrap{
    width: 100%;
  }
  .nearList{
    /* width: 100%; */
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 30rpx;
  }
  .cover{
    width: 180rpx;
    height: 180rpx;
    margin-right: 30rpx;
    margin-left: 10rpx;
  }
  .info {
      flex: 9;
    /* width: 500rpx; */
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: start;
    font-size: 24rpx;
    color: gray;
    padding-right: 20rpx;
  }
  .info text:nth-child(1){
    font-size: 30rpx;
    margin-bottom: 20rpx;
    font-weight: bold;
    color: black;
  }
  .dis{
      flex: 2;
    /* width: 180rpx; */
    /* height: 200rpx; */
    color: #ff4713;
    font-size: 28rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }
  .dh{
    width: 40rpx;
    height: 40rpx;
    margin-top: 10rpx;
  }
  
  .address span{
      font-size: 26rpx;
      margin-right: 20rpx;
  }
  
  .order{
      border-top: 1px solid #ff4713;
      display: flex;
      justify-content: space-around;
  }
  
  .order .border-none{
      border: none;
  }
  
  .subscribe{
      height: 74%;
      background-color:#EDEDED !important;
  }
  
  .card view {
      text-align: center;
      margin: 8px 0;
  }
  
  .card view:last-child{
      font-weight: bold;
  }
  
  .sub{
      padding: 40rpx;
      margin-top: 10rpx;
  }
  
  .subInput{
      border: 1px solid #000;
      padding: 20rpx;
      border-radius: 15rpx !important;
      font-size: 28rpx;
      margin-bottom: 30rpx;
  }
  
